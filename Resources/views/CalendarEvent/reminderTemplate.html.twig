<script type="text/html" class="reminder_templates" data-identifier="calendar_event_template">
    <% if (url != '') { %>
    <span><a class="hash-navigation-link" href="<%- url %>"><%- subject %></a> {{ "oro.calendar.reminder_template.due_message"|trans }} <%- expireAt %></span>
    <% } else { %>
    <span><%- subject %>{{ "oro.calendar.reminder_template.due_message"|trans }} <%- expireAt %></span>
    <% } %>
</script>
